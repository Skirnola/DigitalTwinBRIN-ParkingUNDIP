{
  "version": 2,
  "builds": [
    { "src": "*.html", "use": "@vercel/static" },
    { "src": ".*\\.(css|js|png|jpg|jpeg|gif|svg|ico)", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/style.css", "dest": "/style.css" },
    { "src": "/app.js", "dest": "/app.js" },
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/login", "dest": "/login.html" },
    { "src": "/register", "dest": "/register.html" },
    { "src": "/forgot-password", "dest": "/forgotpassword.html" },
    { "src": "/live-overview", "dest": "/index.html" },
    { "src": "/previous-analysis", "dest": "/previous-analysis.html" },
    { "src": "/profile", "dest": "/profile.html" },
    { "src": "/aboutus", "dest": "/aboutus.html" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/login.html" }
  ]
}